{
  "name": "advent2020",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "author": "Caleb Mennen <cmennen@cisco.com>",
  "license": "MIT",
  "scripts": {
    "start": "tsns run",
    "test": "run-s build-and:test",
    "tdd": "run-p watch:*",
    "build": "tsns build",
    "docker:build": "docker build -t ${npm_package_name}:${npm_package_version} ../../Downloads/typescript-node-starter-master",
    "docker:run": "docker run --rm ${npm_package_name}:${npm_package_version}",
    "commitmsg": "validate-commit-msg",
    "precommit": "run-s lint:* build",
    "prepush": "run-s build-and:test",
    "lint:ts": "tslint --project tsconfig.json",
    "lint-and-fix:-ts": "tslint --project tsconfig.json --fix",
    "build-and:test": "run-s build test-once",
    "build-and:start": "run-s build start",
    "watch:ts": "tsc -w",
    "watch:test": "mocha dist/**/*.test.js -w --colors --inspect --growl",
    "test-once": "mocha dist/**/*.test.js --inspect --colors",
    "contributors-add": "all-contributors add",
    "contributors-generate": "all-contributors generate"
  },
  "dependencies": {
    "tsns": "0.0.18",
    "typescript": "3.x"
  },
  "devDependencies": {
    "@types/chai": "4.x",
    "@types/mocha": "5.x",
    "@types/node": "12.x",
    "all-contributors-cli": "6.x",
    "chai": "4.x",
    "husky": "0.14.x",
    "mocha": "5.x",
    "npm-run-all": "4.x",
    "sinon": "6.x",
    "tslint": "5.x",
    "tslint-config-acamica": "2.x",
    "validate-commit-msg": "2.x"
  }
}
